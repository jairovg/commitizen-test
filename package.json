{
  "name": "commitizen-test",
  "version": "0.0.0",
  "description": "Commitizen setup test",
  "keywords": [
    "commitizen",
    "setup"
  ],
  "author": "jairovg",
  "license": "MIT",
  "devDependencies": {
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true"
    }
  }
}
