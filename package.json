{
  "name": "commitizen-test",
  "version": "0.0.2",
  "private": true,
  "description": "Commitizen setup test",
  "scripts": {
    "release": "HUSKY_SKIP_HOOKS=1 standard-version --no-verify"
  },
  "keywords": [
    "commitizen",
    "setup"
  ],
  "author": "jairovg",
  "license": "MIT",
  "homepage": "https://github.com/jairovg/commitizen-test#readme",
  "repository": {
    "type" : "git",
    "url" : "https://github.com/jairovg/commitizen-test.git"
  },
  "devDependencies": {
    "@commitlint/cli": "^8.3.5",
    "@commitlint/config-conventional": "^8.3.4",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "husky": "^4.2.5",
    "semantic-release": "^17.1.1"
  },
  "config": {
    "commitizen": {
      "path": "cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "prepare-commit-msg": "exec < /dev/tty && git cz --hook || true",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  }
}
